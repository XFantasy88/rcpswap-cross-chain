{
  "name": "@rcpswap/react-query",
  "version": "1.0.0",
  "description": "RCPSwap React Query",
  "keywords": [
    "rcpswap",
    "react",
    "query"
  ],
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@rcpswap/router": "workspace:*",
    "@tanstack/react-query": "4.28.0",
    "@tanstack/react-query-devtools": "4.28.0",
    "idb-keyval": "6.2.0",
    "nanoid": "4.0.0",
    "rcpswap": "workspace:*",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@sentry/nextjs": "7.77.0",
    "@tsconfig/esm": "1.0.4",
    "@tsconfig/strictest": "2.0.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.2.2",
    "viem": "1.17.2",
    "wagmi": "1.4.5"
  },
  "peerDependencies": {
    "@sentry/nextjs": "7.77.0",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "viem": "1.17.2",
    "wagmi": "1.4.5"
  }
}
