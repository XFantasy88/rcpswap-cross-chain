{
  "name": "@rcpswap/router",
  "version": "1.0.0",
  "description": "RCPSwap Router",
  "keywords": [
    "rcpswap",
    "routers"
  ],
  "main": "dist/index.js",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@ethersproject/address": "5.7.0",
    "@rcpswap/router-config": "workspace:*",
    "@rcpswap/tines": "workspace:*",
    "@rcpswap/v2-sdk": "workspace:*",
    "@rcpswap/v3-sdk": "workspace:*",
    "@rcpswap/viem-config": "workspace:*",
    "date-fns": "2.30.0",
    "lodash.flatmap": "^4.5.0",
    "node-fetch": "2.6.6",
    "rcpswap": "workspace:*",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@tsconfig/esm": "1.0.4",
    "@tsconfig/node18": "18.2.2",
    "@tsconfig/strictest": "2.0.2",
    "@types/lodash.flatmap": "4.5.7",
    "@types/node": "18",
    "@types/node-fetch": "2.6.2",
    "typescript": "5.2.2",
    "viem": "1.17.2"
  },
  "peerDependencies": {
    "viem": "1.17.2"
  }
}
